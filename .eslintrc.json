{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:vue/vue3-essential",
        "airbnb-base"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "import/extensions": "off",
        "import/no-unresolved": "off",
        "no-unused-vars": "warn"
    },
    "vue/no-parsing-error": [
        "error",
        {
            "x-invalid-end-tag": false
        }
    ]
}